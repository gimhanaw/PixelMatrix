void BFLotus_1()
{
    cleara8x25();

                  
    //Read_LEDMatrixStudio_Data(ledarray);
    int pos = 0;
    for (byte i =  0; i < LED_Rows; i++) 
    {
        for (byte j = 0; j < LED_Columns; j++) 
        {
            pos = (i * LED_Columns + j) * 3;
            a8x25[i][j][0] = pgm_read_word_near(arrayBFLotus1 + pos); //ledarray[pos];
