    {
      
      StartupFrame_ChangeColor(255,0,0, 0,0,255);
      //Reveal(Up, DELAY, 14);
      
      for (int rep = 0; rep < 4; rep++)
      {
          Reveal(Up, DELAY, 14);
          StartupFrame_FlipLeftRight();
          //Reveal(Up, DELAY, 14);
      }
  
      StartupFrame_ChangeColor(0,0,255, 255,255,0);
      //Reveal(Up, DELAY, 14);
      //StartupFrame_FlipLeftRight();
      for (int rep = 0; rep < 4; rep++)
      {
          Reveal(Up, DELAY, 14);
          StartupFrame_FlipLeftRight();
          //Reveal(Up, DELAY, 14);
      }
      
      StartupFrame_ChangeColor(255,255,0, 255,0,0);
      //Reveal(Up, DELAY, 14);
      //StartupFrame_FlipLeftRight();
