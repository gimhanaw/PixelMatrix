      {
          Reveal(Up, DELAY, 14);
          StartupFrame_FlipLeftRight();
          
      }
      
      StartupFrame_ChangeColor(255,0,0, 200,200,200);
      //Reveal(Up, DELAY, 14);
      //StartupFrame_FlipLeftRight();
