void RedPieces()
{
    cleara8x25();
        
    //Read_LEDMatrixStudio_Data(arrayZigZagBlue);
    int pos = 0;
    for (byte i =  0; i < LED_Rows; i++) 
    {
        for (byte j = 0; j < LED_Columns; j++) 
        {
            pos = (i * LED_Columns + j) * 3;
            a8x25[i][j][0] = pgm_read_word_near(arrayRedPieces + pos); 
            a8x25[i][j][1] = pgm_read_word_near(arrayRedPieces + pos+1);
            a8x25[i][j][2] = pgm_read_word_near(arrayRedPieces + pos+2);
        }
    }
    if (Data_Input == Inside)
      StartupFrame_FlipUpsideDown();
}
