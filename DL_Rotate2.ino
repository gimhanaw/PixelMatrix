void Run_DL_Rotate2()
{
    LYclearmainarray(1);
    LYclearmainarray(2);
    clearmainarray();
    
    LYRead_LEDMatrixStudio_Data(1, DL_Rotate2_L1); // Layer1
    LYRead_LEDMatrixStudio_Data(2, DL_Rotate2_L2); // Layer2

    int Delay = 60;
      
    RevealLayers(Up, Down, Delay, 14);
    AnimateLayers(Right, RightDown, Delay, 140);
    WipeLayers(Down, Up, Delay, 14);

    RevealLayers(Up, Down, Delay, 14);
    AnimateLayers(Left, LeftDown, Delay, 140);
    WipeLayers(Up, Down, Delay, 14);

    Delay = DELAY;
}

void Run_DL_Rotate2_2()
{
    LYclearmainarray(1);
    LYclearmainarray(2);
    clearmainarray();
    
    LYRead_LEDMatrixStudio_Data(1, DL_Rotate2_L1); // Layer1
    LYRead_LEDMatrixStudio_Data(2, DL_Rotate2_L2); // Layer2

    int Delay = 60;
    
    RevealLayers(Up, Down, Delay, 14);
    AnimateLayers(Up, Down, Delay, 140);
    WipeLayers(Up, Down, Delay, 7);

    //LYStartupFrame_FlipUpsideDown(1);
    LYStartupFrame_FlipUpsideDown(2);
    RevealLayers(Down, Up, Delay, 14);
    AnimateLayers(Down, Up, Delay, 140);
    WipeLayers(Down, Up, Delay, 7);

    Delay = DELAY;
}

void Run_DL_Rotate2_Bounce()
{
    DELAY = 70;
    Read_LEDMatrixStudio_Data(DL_Rotate2_L1); // Layer1 Used as startup frame

    Reveal(Up, DELAY, 14);

    for (int i = 0; i < 35; i++)
    {
      Animate(RightDown, DELAY, 7);
      Animate(RightUp, DELAY, 7);
    }
    Wipe(Up, DELAY, 7);
    DELAY = 40;

}
