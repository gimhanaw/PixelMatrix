void Run_RainbowButterfly()
{
    
    for (int rep = 0; rep < 15; rep++)
    {
        RainbowButterfly1();
        Reveal(Up, DELAY, 14);
        RainbowButterfly2();
        Reveal(Up, DELAY, 14);
        RainbowButterfly3();
        Reveal(Up, DELAY, 14);
        RainbowButterfly4();
        Reveal(Up, DELAY, 14);
        Wipe(Up, DELAY, 14);
    }
}



void RainbowButterfly1()
{
    cleara8x25();
        
    int pos = 0;
    for (byte i =  0; i < LED_Rows; i++) 
    {
        for (byte j = 0; j < LED_Columns; j++) 
        {
            pos = (i * LED_Columns + j) * 3;
            a8x25[i][j][0] = pgm_read_word_near(arrayRainbowButterfly1 + pos); 
            a8x25[i][j][1] = pgm_read_word_near(arrayRainbowButterfly1 + pos+1); 
            a8x25[i][j][2] = pgm_read_word_near(arrayRainbowButterfly1 + pos+2);
        }
    }
    if (Data_Input == Inside)
      StartupFrame_FlipUpsideDown();
}

