void Run_BlueLotus(int Loops, int Delay)
{
    for (int i=0; i < Loops; i++)
      {
          BlueLotus_1();
          Reveal(Up, Delay, LED_Rows);
          BlueLotus_2();
          Reveal(Up, Delay, LED_Rows);
      }
}



void BlueLotus_1()
{
    cleara8x25();

    int pos = 0;
    for (byte i =  0; i < LED_Rows; i++) 
    {
        for (byte j = 0; j < LED_Columns; j++) 
        {
            pos = (i * LED_Columns + j) * 3;
            a8x25[i][j][0] = pgm_read_word_near(arrayBlueLotus1 + pos); 
            a8x25[i][j][1] = pgm_read_word_near(arrayBlueLotus1 + pos+1); 
            a8x25[i][j][2] = pgm_read_word_near(arrayBlueLotus1 + pos+2); 
        }
    }
    if (Data_Input == Inside)
      StartupFrame_FlipUpsideDown();
}

